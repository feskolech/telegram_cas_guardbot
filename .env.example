BOT_TOKEN=REPLACE_ME
DB_PATH=/data/bot.sqlite3
BANNED_LOG_PATH=/data/banned.txt

# recheck "seen" users
RECHECK_INTERVAL=1m

# refresh sources (both lists)
UPDATE_EXPORT_INTERVAL=30m
UPDATE_LOLS_INTERVAL=30m

# how many message_id to keep per user for deletions
MESSAGE_CACHE_LIMIT=50

# how many days to keep seen_users (then prune)
SEEN_TTL_DAYS=7

# HTTP timeouts
HTTP_TIMEOUT_SECONDS=7

# CAS API check cache TTL
CAS_CACHE_TTL=10m
